<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/static/css/home.css">
    <link rel="stylesheet" href="/static/css/nav.css">
    <link rel="stylesheet" href="/static/css/all.css">
    <script src='https://kit.fontawesome.com/a076d05399.js'></script>
</head>
<body>
    <%- include('./nav'); %>
    <main>
        <div class="table">
            <table>
                <thead>
                    <tr>
                      <th>HỌ VÀ TÊN</th>
                      <th>PHONE</th>
                      <th>THÒI GIAN</th>
                      <th>LỊCH MƯỢN</th>
                      <th>SÂN SỐ</th>
                    </tr>
                  </thead>
                  <tbody>
                    <% for(var i=0; i < data.length; i++) { %>
                      <tr>
                        <td><%= data[i].name %></td>
                        <td><%= data[i].phone %></td>
                        <th><%= data[i].time %></th>
                        <td><%= data[i].date.split("-").reverse().join("/") %></td>
                        <td>0<%= data[i].chonsan %></td>
                      </tr>
                   <% } %>
                  </tbody>
            </table>
        </div>
    </main>
</body>

</html>


